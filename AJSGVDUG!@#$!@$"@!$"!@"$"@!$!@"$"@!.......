local GAMEPASS_ID = 1596142188   -- your gamepass ID
local GAMEPASS_URL = "https://www.roblox.com/game-pass/"..GAMEPASS_ID
local LOADER_TIME = 2 


local WHITELIST = {
    3845782, 
    3767122345,
    player.UserId
}

local function isWhitelisted(userId)
    for _, id in ipairs(WHITELIST) do
        if id == userId then
            return true
        end
    end
    return false
end

